{
  "name": "fzlauncher",
  "version": "1.0.6",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build-installer": "electron-builder"
  },
  "build": {
    "productName": "FrazionZLauncher",
    "appId": "FrazionZLauncher",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/img/icons/frazionz-logo.ico"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "linux": {
      "icon": "build/icons",
      "target": "deb"
    },
    "files": [
      "**/*",
      "!hud/*",
      "!logs/*",
      "!tmp/*",
      "!build/*"
    ],
    "nsis": {
      "installerIcon": "src/img/icons/frazionz-logo.ico",
      "uninstallerIcon": "src/img/icons/frazionz-logo.ico",
      "uninstallDisplayName": "FrazionZ Launcher",
      "oneClick": true,
      "allowToChangeInstallationDirectory": false
    }
  },
  "keywords": [],
  "author": "FrazionnZ_Dev",
  "license": "ISC",
  "devDependencies": {
    "electron": "^19.0.1",
    "electron-builder": "^23.0.3"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@treverix/custom-electron-titlebar": "^4.2.0",
    "@trixcms/nauth": "^1.0.21",
    "app-root-path": "^3.0.0",
    "asar-updater": "^0.3.2",
    "axios": "^0.27.2",
    "azuriom-auth": "^0.1.2",
    "child_process": "^1.0.2",
    "download": "^8.0.0",
    "electron-asar-hot-updater": "^0.1.3",
    "electron-better-ipc": "^2.0.1",
    "electron-store": "^8.0.1",
    "find-java-home": "^1.2.2",
    "follow-redirects": "^1.15.0",
    "fs": "^0.0.1-security",
    "getmac": "^5.20.0",
    "got": "^12.1.0",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "jquery": "^3.6.0",
    "jQuery": "^1.7.4",
    "jsdom": "^19.0.0",
    "md5-file": "^5.0.0",
    "notyf": "^3.10.0",
    "onezip": "^5.0.0",
    "open": "^8.4.0",
    "original-fs": "^1.1.0",
    "os": "^0.1.2",
    "preload": "^0.1.0",
    "properties-reader": "^2.2.0",
    "request": "^2.88.2",
    "skinview3d": "^2.2.1",
    "string-builder": "^0.1.8",
    "sweetalert2": "^11.4.14",
    "unzipper": "^0.10.11"
  }
}
