{
  "name": "fzlauncher",
  "version": "1.3.95",
  "description": "",
  "email": "florent.cala@gmail.com",
  "main": "main.js",
  "scripts": {
    "start": "electron --trace-deprecation .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "build-installer": "electron-builder"
  },
  "outputCapture": "std",
  "build": {
    "productName": "FrazionZ Launcher",
    "appId": "FrazionZ Launcher",
    "directories": {
      "output": "build"
    },
    "protocols": {
      "name": "fzlauncher-protocol",
      "schemes": [
        "fzlauncher"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/assets/img/icons/frazionz-logo.ico"
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "linux": {
      "icon": "src/assets/img/icons/icon-256.png",
      "category": "Game",
      "target": "deb"
    },
    "files": [
      "**/*",
      "!hud/*",
      "!logs/*",
      "!tmp/*",
      "!build/*"
    ],
    "nsis": {
      "installerIcon": "src/assets/img/icons/frazionz-logo.ico",
      "uninstallerIcon": "src/assets/img/icons/frazionz-logo.ico",
      "uninstallDisplayName": "FrazionZ Launcher",
      "oneClick": true,
      "allowToChangeInstallationDirectory": false
    }
  },
  "keywords": [],
  "author": "FrazionZ_Dev <florent.cala@gmail.com> ",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^20.0.0",
    "electron-builder": "^23.0.3",
    "javascript-obfuscator": "^4.0.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@futurelucas4502/light-electron-renderer": "^1.0.3",
    "@sentry/electron": "^4.0.0",
    "adm-zip": "^0.5.9",
    "app-root-path": "^3.0.0",
    "archiver": "^5.3.1",
    "axios": "^0.27.2",
    "azuriom-auth": "^0.1.2",
    "base-64": "^1.0.0",
    "byte-size": "^8.1.0",
    "check-internet-connected": "^2.0.6",
    "child_process": "^1.0.2",
    "crypto": "^1.0.1",
    "ejs": "^3.1.8",
    "electron-asar-hot-updater": "^0.1.3",
    "electron-better-ipc": "^2.0.1",
    "electron-is-packaged": "^1.0.2",
    "electron-root-path": "^1.0.16",
    "electron-store": "^8.0.1",
    "extract-zip": "^2.0.1",
    "fetch": "^1.1.0",
    "find-java-home": "^1.2.2",
    "follow-redirects": "^1.15.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.1.0",
    "got": "^12.1.0",
    "handlebars": "^4.7.7",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "image-to-base64": "^2.2.0",
    "jquery": "^3.6.0",
    "jQuery": "^1.7.4",
    "jsdom": "^19.0.0",
    "jszip": "^3.10.1",
    "log-timestamp": "^0.3.0",
    "markdown-it": "^13.0.1",
    "md5-file": "^5.0.0",
    "minecraft-launcher-core": "^3.16.16",
    "mousetrap": "^1.6.5",
    "node-fetch": "^2.6.7",
    "node-stream-zip": "^1.15.0",
    "notyf": "^3.10.0",
    "onezip": "^5.0.0",
    "open": "^8.4.0",
    "original-fs": "^1.1.0",
    "os": "^0.1.2",
    "preload": "^0.1.0",
    "properties-reader": "^2.2.0",
    "readline": "^1.3.0",
    "request": "^2.88.2",
    "skinview3d": "^2.2.1",
    "string-builder": "^0.1.8",
    "sweetalert2": "^11.4.14",
    "underscore": "^1.13.6",
    "unzipper": "^0.10.11",
    "user-agents": "^1.0.1136",
    "uuid": "^8.3.2"
  }
}
